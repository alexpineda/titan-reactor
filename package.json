        "process-replay": "0.0.11",
        "random": "^3.0.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet": "^6.1.0",
        "react-markdown": "^8.0.0",
        "sanitize-filename": "^1.6.3",
        "scm-extractor": "^1.0.6",
        "semver": "^7.3.5",
        "three": "0.156.0",
        "three-gpu-pathtracer": "^0.0.6",
        "three-instanced-uniforms-mesh": "^0.46.0",
        "three-janitor": "^2.0.1",
        "typescript": "^4.8.4",
        "winreg": "^1.2.4",
        "zustand": "4.1.1"
    },
    "devDependencies": {
        "@11ty/eleventy": "^2.0.1",
