{
    "name": "titan-reactor",
    "productName": "Titan Reactor",
    "version": "0.4.5",
    "description": "The Advanced Starcraft Broodwar Observing Tool",
    "main": "src/main/index.js",
    "scripts": {
        "test": "jest --watchAll",
        "test:once": "jest --runInBand",
        "test:debug": "node --inspect node_modules/jest/bin/jest --runInBand",
        "clear-cache": "rimraf .cache dist",
        "build": "electron-webpack",
        "dist": "yarn build && electron-builder",
        "dist:dir": "yarn dist -- --dir -c.compression=store -c.mac.identity=null",
        "dev": "electron-webpack dev",
        "dist:run": "yarn build && electron dist/main/main.js",
        "lint": "yarn eslint .",
        "clean-dev-win": "taskkill /f /im node.exe",
        "postinstall": "git apply -v --stat --directory=node_modules/scm-extractor build/patches/scm-extractor.patch"
    },
    "browserslist": [
        "last 2 Chrome versions"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/imbateam-gg/titan-reactor.git"
    },
    "author": "Alex Pineda <alexpineda@fastmail.com>",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/imbateam-gg/titan-reactor/issues"
    },
    "homepage": "https://github.com/imbateam-gg/titan-reactor#readme",
    "electronWebpack": {
        "commonSourceDirectory": "src/common",
        "staticSourceDirectory": "bundled",
        "title": "Titan Reactor",
        "main": {
            "sourceDirectory": "src/main"
        },
        "renderer": {
            "sourceDirectory": "src/renderer",
            "webpackConfig": "build/renderer.config.js"
        },
        "whiteListedModules": [
            "zustand",
            "leva"
        ]
    },
    "build": {
        "appId": "imbateam.titan-reactor",
        "productName": "Titan Reactor",
        "mac": {
            "category": "The Advanced Observing App"
        },
        "extraResources": [
            {
                "from": "bundled",
                "to": "bundled"
            }
        ]
    },
    "dependencies": {
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.14.5",
        "@npmcli/package-json": "^1.0.1",
        "assert": "^2.0.0",
        "bl": "^5.0.0",
        "bw-casclib": "^2.0.0",
        "bw-chk": "^1.3.0",
        "byte-size": "^8.1.0",
        "camera-controls": "^1.33.2",
        "concat": "1.0.3",
        "d3-ease": "1.0.6",
        "deepmerge": "^4.2.2",
        "downgrade-replay": "0.0.9",
        "electron-devtools-installer": "^3.2.0",
        "electron-is-dev": "^1.2.0",
        "express": "^4.17.3",
        "iconv-lite": "0.6.3",
        "leva": "^0.9.23",
        "libnpmsearch": "^5.0.0",
        "lodash.debounce": "^4.0.8",
        "lodash.get": "^4.4.2",
        "lodash.groupby": "^4.6.0",
        "lodash.shuffle": "^4.2.0",
        "mkdirp": "^1.0.4",
        "muicss": "^0.10.3",
        "pacote": "^13.0.3",
        "postprocessing": "6.24.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-jsx-parser": "^1.29.0",
        "react-markdown": "^8.0.0",
        "remark-gfm": "^3.0.1",
        "sanitize-filename": "^1.6.3",
        "scm-extractor": "^1.0.6",
        "semver": "^7.3.5",
        "three": "0.137.0",
        "webpack": "4.42.1",
        "winreg": "^1.2.4",
        "zustand": "3.3.3"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.15.8",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/preset-typescript": "^7.15.0",
        "@types/babel__core": "^7.1.18",
        "@types/bl": "^5.0.2",
        "@types/buble": "^0.20.1",
        "@types/concat-stream": "^1.6.1",
        "@types/d3-ease": "^3.0.0",
        "@types/dompurify": "^2.3.3",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.3",
        "@types/js-yaml": "^4.0.5",
        "@types/libnpmsearch": "^2.0.3",
        "@types/lodash.debounce": "^4.0.6",
        "@types/lodash.get": "^4.4.6",
        "@types/lodash.groupby": "^4.6.6",
        "@types/lodash.shuffle": "^4.2.6",
        "@types/mkdirp": "^1.0.2",
        "@types/muicss": "^0.9.4",
        "@types/pacote": "^11.1.3",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/semver": "^7.3.9",
        "@types/three": "^0.133.1",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "babel-jest": "^26.3.0",
        "babel-loader": "^8.2.2",
        "benny": "^3.7.1",
        "dotenv": "^8.2.0",
        "electron": "15.3.5",
        "electron-builder": "^22.8.1",
        "electron-webpack": "^2.8.2",
        "electron-webpack-ts": "^4.0.1",
        "eslint": "^7.16.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jest": "^25.2.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0",
        "glslify-loader": "^2.0.0",
        "jest": "^27.2.4",
        "prettier": "^2.4.1",
        "raw-loader": "^4.0.2",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.19",
        "typescript": "^4.4.4",
        "worker-loader": "^2.0.0"
    }
}
