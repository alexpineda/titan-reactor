{
    "name": "titan-reactor",
    "productName": "Titan Reactor",
    "version": "0.6.5",
    "description": "An OpenBW Renderer",
    "main": "dist/electron/main/index.js",
    "scripts": {
        "dev": "vite",
        "sanity": "yarn typecheck && yarn test:once",
        "typecheck": "tsc --noEmit",
        "test": "jest --watchAll",
        "test:once": "jest --runInBand",
        "test:debug": "node --inspect node_modules/jest/bin/jest --runInBand",
        "clear-cache": "rimraf .cache dist",
        "build": "vite build",
        "dist": "vite build && electron-builder",
        "lint": "yarn eslint .",
        "clean-dev-win": "taskkill /f /im node.exe",
        "postinstall": "git apply -v --stat --directory=node_modules/scm-extractor build/patches/scm-extractor.patch",
        "build-api-types": "ts-node -T .\\build\\apis\\build-api-types.ts"
    },
    "browserslist": [
        "last 2 Chrome versions"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/imbateam-gg/titan-reactor.git"
    },
    "author": "Alex Pineda <alexpineda@fastmail.com>",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/imbateam-gg/titan-reactor/issues"
    },
    "homepage": "https://github.com/imbateam-gg/titan-reactor#readme",
    "build": {
        "directories": {
            "output": "release",
            "buildResources": "build"
        },
        "files": [
            "dist"
        ],
        "appId": "imbateam.titan-reactor",
        "productName": "Titan Reactor",
        "mac": {
            "category": "An OpenBW Renderer"
        },
        "linux": {
            "category": "Game",
            "icon": "icon256x256.ico"
        },
        "extraResources": [
            {
                "from": "bundled",
                "to": "bundled"
            }
        ]
    },
    "debug": {
        "env": {
            "VITE_DEV_SERVER_HOSTNAME": "127.0.0.1",
            "VITE_DEV_SERVER_PORT": 3344,
            "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
        }
    },
    "dependencies": {
        "@codemirror/lang-javascript": "^6.1.0",
        "@npmcli/package-json": "^2.0.0",
        "@types/lodash.set": "^4.3.7",
        "@types/rbush": "^3.0.0",
        "@typescript/vfs": "^1.4.0",
        "bl": "^5.0.0",
        "bw-casclib": "^2.0.1",
        "bw-chk": "^1.3.0",
        "camera-controls": "1.37.2",
        "codemirror": "^6.0.1",
        "concat-stream": "^2.0.0",
        "deep-diff": "^1.0.2",
        "deepmerge": "^4.2.2",
        "electron-devtools-installer": "^3.2.0",
        "electron-is-dev": "^2.0.0",
        "express": "^4.17.3",
        "iconv-lite": "0.6.3",
        "immer": "^9.0.15",
        "leva": "^0.9.23",
        "libnpmsearch": "^5.0.0",
        "lodash.debounce": "^4.0.8",
        "lodash.get": "^4.4.2",
        "lodash.groupby": "^4.6.0",
        "lodash.omit": "^4.5.0",
        "lodash.set": "^4.3.2",
        "lodash.shuffle": "^4.2.0",
        "lodash.throttle": "^4.1.1",
        "mkdirp": "^1.0.4",
        "node-fetch": "3.2.10",
        "pacote": "^14.0.0",
        "pkware-wasm": "^1.0.0",
        "postprocessing": "6.28.7",
        "random": "^3.0.6",
        "rbush": "^3.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet": "^6.1.0",
        "react-markdown": "^8.0.0",
        "sanitize-filename": "^1.6.3",
        "scm-extractor": "^1.0.6",
        "semver": "^7.3.5",
        "ses": "^0.15.17",
        "three": "0.145.0",
        "three-gpu-pathtracer": "^0.0.6",
        "three-instanced-uniforms-mesh": "^0.46.0",
        "three-janitor": "^2.0.1",
        "threejs-shader-materials": "MasatoMakino/threejs-shader-materials",
        "troika-three-utils": "^0.46.0",
        "ts-morph": "^16.0.0",
        "typescript": "^4.8.4",
        "winreg": "^1.2.4",
        "zustand": "4.1.1"
    },
    "devDependencies": {
        "@types/bl": "^5.0.2",
        "@types/concat-stream": "^2.0.0",
        "@types/d3-ease": "^3.0.0",
        "@types/deep-diff": "^1.0.1",
        "@types/express": "^4.17.13",
        "@types/jest": "^29.0.3",
        "@types/libnpmsearch": "^2.0.3",
        "@types/lodash.debounce": "^4.0.6",
        "@types/lodash.get": "^4.4.6",
        "@types/lodash.groupby": "^4.6.6",
        "@types/lodash.omit": "^4.5.6",
        "@types/lodash.shuffle": "^4.2.6",
        "@types/lodash.throttle": "^4.1.6",
        "@types/mkdirp": "^1.0.2",
        "@types/node": "^18.7.1",
        "@types/pacote": "^11.1.3",
        "@types/react": "18.0.21",
        "@types/react-dom": "18.0.6",
        "@types/react-helmet": "^6.1.5",
        "@types/semver": "^7.3.9",
        "@types/three": "^0.144.0",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "@vitejs/plugin-react": "^2.1.0",
        "dotenv": "^16.0.3",
        "electron": "21.1.0",
        "electron-builder": "^23.3.3",
        "eslint": "^8.24.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jest": "^27.1.1",
        "eslint-plugin-jest-extended": "^2.0.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0",
        "jest": "^29.0.3",
        "jest-environment-jsdom": "^29.0.3",
        "jest-extended": "^3.1.0",
        "prettier": "^2.4.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.19",
        "spectorjs": "^0.9.27",
        "ts-jest": "^29.0.1",
        "ts-node": "^10.9.1",
        "vite": "^3.1.3",
        "vite-plugin-electron": "^0.9.3",
        "vite-plugin-esmodule": "^1.4.2"
    },
    "config": {
        "titan-reactor-api": "2.0.0"
    }
}
